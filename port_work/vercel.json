{
    "version": 2,
    "builds": [
      {
        "src": "landing.html",
        "use": "@vercel/static"
      },
      {
        "src": "work.html",
        "use": "@vercel/static"
      },
      {
        "src": "style.css",
        "use": "@vercel/static"
      }
    ],
    "routes": [
      {
        "src": "/",
        "dest": "/landing.html"
      },
      {
        "src": "/work",
        "dest": "/work.html"
      }
    ],
    "env": {
      "PROJECT_NAME": "port_work"
    }
  }
  